class Vendedor extends Funcionario {
    public Vendedor(String nome, int id) {
        super(nome, id);
    }

    public void realizarVenda() {
        System.out.println(getNome() + " está realizando uma venda.");
    }

    public void solicitarMaterial() {
        System.out.println(getNome() + " está solicitando material para reposição.");
    }
}
